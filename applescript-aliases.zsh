# Edit iTerm scripts
alias itermConfig="code ~/Library/Application\ Support/iTerm2/Scripts"

# Backup iTerm scripts in iCloud
alias itermBackup="cp ~/Library/Application\ Support/iTerm2/Scripts/* ~/Documents/scripts/iTerm2-scripts"

# Run iTerm scripts
alias darkish="darkishTab.scpt"
alias enclients="enclientsTab.scpt"
alias enbff="bffTab.scpt"
